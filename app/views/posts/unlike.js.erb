document.querySelector("#like_<%= @post.id %>").classList.remove("like_fill");
document.querySelector("#like_<%= @post.id %>").classList.add("like_non_fill");

document.querySelector("#like_<%= @post.id %>").href = "/posts/<%= @post.id %>/like"

document.querySelector("#likes_<%= @post.id %>").innerHTML = '<%= display_likes @post %>';